{
  "name": "wsol-anchor",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "generate codama client": "codama run --all",
    "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
    "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check",
    "build": "anchor build && codama run --all",
    "test": "anchor test",
    "1. download metadata_program": "solana program dump -u m metaqbxxUerdq28cj1RbAWkYQm3ybzjb6a8bt518x1s token_metadata_program.so",
    "2. local network": "solana-test-validator --bpf-program metaqbxxUerdq28cj1RbAWkYQm3ybzjb6a8bt518x1s token_metadata_program.so --reset",
    "3. test-local": "anchor test --skip-local-validator",
    "deploy devnet": "anchor deploy --provider.cluster devnet",
    "update upgrade-authority": "solana program set-upgrade-authority --skip-new-upgrade-authority-signer-check wso1PkvZVRh2KSdrhBeFFd15E36ggcwuwp8qmdqDVjn --upgrade-authority ~/.config/solana/id.json --new-upgrade-authority 6HmQJ6bn3AExJwKvdWZbN96kEJcKTgTnZ4szM5atYyC8"
  },
  "dependencies": {
    "@codama/nodes-from-anchor": "^1.2.9",
    "@codama/renderers-js": "^1.4.3",
    "@coral-xyz/anchor": "0.32.1",
    "@metaplex-foundation/mpl-token-metadata": "^3.2.1",
    "@metaplex-foundation/umi": "^0.9.2",
    "@metaplex-foundation/umi-bundle-defaults": "^0.9.2",
    "@solana-program/token": "^0.7.0",
    "@solana/kit": "^4.0.0",
    "@solana/spl-token": "^0.4.8",
    "bs58": "^6.0.0",
    "codama": "^1.3.7",
    "solana-kite": "^1.7.1",
    "tsx": "^4.20.6"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^24.7.2",
    "chai": "^4.3.4",
    "mocha": "^9.0.3",
    "prettier": "^2.6.2",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.3.5"
  }
}
